<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>Stenn.MSBuild.Extensions</id>
        <version>$version$</version>
        <authors>Stenn International</authors>
        <developmentDependency>true</developmentDependency>
        <license type="expression">MIT</license>
        <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
        <icon>icon.png</icon>
        <readme>README.md</readme>
        <projectUrl>https://github.com/StennGroup/stenn-msbuild-extensions</projectUrl>
        <iconUrl>https://github.com/StennGroup/stenn-aspnetcore-versioning/blob/logo/icon.png</iconUrl>
        <description>MSBuild common routine.</description>
        <copyright>$Copyright$</copyright>
        <tags>MSBuild Tasks</tags>
        <dependencies>
                <dependency id="Microsoft.SourceLink.GitHub" version="1.1.1" />
        </dependencies>
    </metadata>
    <files>
        <file src="$ProjectDirectory$build\*.*" target="build" />
        <file src="$ProjectDirectory$buildMultiTargeting\*.*" target="buildMultiTargeting" />
        <file src="$ProjectDirectory$..\..\README.md" />
        <file src="$ProjectDirectory$..\..\LICENSE" />
        <file src="$ProjectDirectory$..\..\logo\icon.png" />
    </files>
</package>
